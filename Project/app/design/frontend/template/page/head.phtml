<?php
$js = $this->getJs();
$css = $this->getCss();
if (count($js) > 0) {
    foreach ($js as $file) {
        echo "<script src={$this->getJsUrl($file)}></script>";
    }
}
if (count($css) > 0) {
    foreach ($css as $file) {
        echo "<link rel='stylesheet' href={$this->getCssUrl($file)}>";
    }
}
